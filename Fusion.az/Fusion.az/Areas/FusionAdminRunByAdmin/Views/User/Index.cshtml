@model List<AppUser>
@{
    ViewData["Title"] = "Index";
}

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Users Table</h4>
            
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>
                                Name
                            </th>
                            <th>
                                Surname
                            </th>

                            <th>
                                Email
                            </th>
                            <th>
                                Phone
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        
                       
                            @foreach (AppUser users in Model)
                            {
                                <tr>
                                    <td class="py-1">
                                       <p>@users.Name</p>
                                    </td>
                                    <td>
                                        <p>@users.Surname</p>
                                    </td>

                                    <td>

                                        <p>@users.Email</p>
                                    </td>
                                    <td>
                                        
                                        <p>@users.PhoneNumber</p>

                                    </td>
                                </tr>
                            }


                        
                       
                        

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
